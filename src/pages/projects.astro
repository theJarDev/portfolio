---
import Layout from "../layouts/Layout.astro";
import SectionContainer from "../components/SectionContainer.astro";
import ProjectCard from "../components/ProjectCard.astro";
import PageTitle from "../components/PageTitle.astro";
//images
import skillsflix from '../assets/Skillsflix.webp';
import skillsflix2 from '../assets/skillsflix2.webp';
import alurageek from '../assets/AluraGeek.webp';
import alurageek2 from '../assets/alurageek2.webp';
import org from '../assets/org.webp';
import org2 from '../assets/org2.webp';
---

<Layout title="Projects">
    <PageTitle page="projects" description="List of all my projects" />
    <SectionContainer title="web-development" viewAll={false} href="">
        <ProjectCard
			slot='projects'
			img1={skillsflix}
			img2={skillsflix2}
			tecnologies='HTML React Styled-Components Material-UI'
			title='SkillsFlix'
			description='SPA video platform. Oracle Next Education Challenge'
			link='https://thejardev.github.io/alura-geek/'
			github='https://github.com/theJarDev/skillsflix'
		/>
		<ProjectCard
			slot='projects'
			img1={alurageek}
			img2={alurageek2}
			tecnologies='HTML CSS JavaScript'
			title='AluraGeek'
			description='E-commerce. Oracle Next Education Challenge'
			link='https://thejardev.github.io/alura-geek/'
			github='https://github.com/theJarDev/alura-geek'
		/>
		<ProjectCard
			slot='projects'
			img1={org}
			img2={org2}
			tecnologies='HTML CSS React'
			title='Org'
			description='App for team organization. Oracle Next Education Challenge'
			link='https://thejardev.github.io/alura-geek/'
			github='https://github.com/theJarDev/alura-geek'
		/>
    </SectionContainer>
    <SectionContainer title="others" viewAll={false} href="">
        <ProjectCard
			slot='projects'
			img1={skillsflix}
			img2={skillsflix2}
			tecnologies='HTML React Styled-Components Material-UI'
			title='SkillsFlix'
			description='SPA video platform. Oracle Next Education Challenge'
			link='https://thejardev.github.io/alura-geek/'
			github='https://github.com/theJarDev/skillsflix'
		/>
		<ProjectCard
			slot='projects'
			img1={alurageek}
			img2={alurageek2}
			tecnologies='HTML CSS JavaScript'
			title='AluraGeek'
			description='E-commerce. Oracle Next Education Challenge'
			link='https://thejardev.github.io/alura-geek/'
			github='https://github.com/theJarDev/alura-geek'
		/>
		<ProjectCard
			slot='projects'
			img1={org}
			img2={org2}
			tecnologies='HTML CSS React'
			title='Org'
			description='App for team organization. Oracle Next Education Challenge'
			link='https://thejardev.github.io/alura-geek/'
			github='https://github.com/theJarDev/alura-geek'
		/>
    </SectionContainer>
</Layout>